<Window x:Class="UIProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UIProject"
        xmlns:lo="clr-namespace:UIProject.Control"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" KeyDown="Window_KeyDown" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
            
        <lo:HAbutton Background="Black" x:Name="btkk" Grid.Column="0" Width="100" Height="100"/>
        <lo:KKButton Margin="100" Grid.Column="1" x:Name="btk2" Width="100" Height="100"/>
   
    </Grid>
</Window>
